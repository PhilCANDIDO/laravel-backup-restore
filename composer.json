{
    "name": "mydatabase/laravel-backup-restore",
    "description": "Enterprise-grade backup and restore solution for Laravel applications with S3 support, encryption, and staging environment creation",
    "type": "library",
    "keywords": [
        "laravel",
        "backup",
        "restore",
        "database",
        "s3",
        "encryption",
        "staging",
        "disaster-recovery"
    ],
    "homepage": "https://github.com/mydatabase/laravel-backup-restore",
    "license": "MIT",
    "authors": [
        {
            "name": "MyDatabase Team",
            "email": "dev@mydatabase.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0",
        "symfony/process": "^6.0|^7.0",
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.6",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MyDatabase\\BackupRestore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyDatabase\\BackupRestore\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyDatabase\\BackupRestore\\BackupRestoreServiceProvider"
            ],
            "aliases": {
                "Backup": "MyDatabase\\BackupRestore\\Facades\\Backup"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}